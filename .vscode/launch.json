    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Cli Get Program",
            "program": "${workspaceFolder}\\bin\\arte.js",
            "args": [
                "get",
                "-b",
                "bucket",
                "-n",
                "name",
                "--metadata.arch",
                "x86"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Cli Put Program",
            "program": "${workspaceFolder}\\bin\\arte.js",
            "args": [
                "put",
                "file.zip",
                "-b",
                "bucket",
                "-n",
                "name",
                "--metadata.arch",
                "x86"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Cli Search Program",
            "program": "${workspaceFolder}\\bin\\arte.js",
            "args": [
                "search",                
                "-b",
                "bucket",
                "-n",
                "name"
            ]
        }
    ]
}